{
    "dashboard": {
      "id": null,
      "uid": null,
      "title": "Tokeping MTR Dashboard",
      "timezone": "browser",
      "schemaVersion": 30,
      "version": 1,
      "templating": {
        "list": [
          {
            "name": "MTR_HOP",
            "type": "query",
            "label": "Hop",
            "datasource": "InfluxDB",
            "query": "import \"influxdata/influxdb/schema\"\\nschema.tagValues(bucket: \\\"metrics\\\", tag: \\\"probe\\\", start: -30d, predicate: (r) => r._measurement == \\\"latency\\\")",
            "regex": "/^mtr-.*$/",
            "refresh": 1,
            "multi": false,
            "includeAll": false
          }
        ]
      },
      "panels": [
        {
          "id": 1,
          "type": "timeseries",
          "title": "Latency for ${MTR_HOP}",
          "repeat": "MTR_HOP",
          "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 },
          "datasource": "InfluxDB",
          "targets": [
            {
              "refId": "A",
              "queryType": "flux",
              "query": "from(bucket: \\\"metrics\\\")\\n  |> range(start: -1h)\\n  |> filter(fn: (r) => r._measurement == \\\"latency\\\" and r._field == \\\"value\\\" and r.probe == v.MTR_HOP)\\n  |> aggregateWindow(every: $__interval, fn: mean, createEmpty: false)\\n  |> yield(name: \\\"mean\\\")"
            }
          ],
          "fieldConfig": {
            "defaults": { "unit": "ms" },
            "overrides": []
          },
          "options": {
            "legend": { "displayMode": "table", "placement": "bottom" }
          }
        }
      ]
    },
    "overwrite": false
  }
  